		<tbody>
			<tr>
				<th>
					原密码:
				</th>
				<td>
					<input type="text" name='oldpwd' id='oldpwd' />
				</td>
			</tr>
			<tr>
				<th>
					新密码:
				</th>
				<td>
					<input type="text" name='password' id='password' />
				</td>
			</tr>
			<tr>
				<th>
					确认新密码:
				</th>
				<td>
					<input type="text" name='confirmPwd' id='confirmPwd' />
				</td>
			</tr>
		</tbody>
	</table>
</form>