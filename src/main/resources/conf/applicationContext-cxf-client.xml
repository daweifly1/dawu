<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<bean id="helloWorldClient" class="net.swa.dewei.service.ws.HelloWorld"   
      factory-bean="helloWorldClientFactory" factory-method="create"/>  
      
    <bean id="helloWorldClientFactory" class="org.apache.cxf.jaxws.JaxWsProxyFactoryBean">  
      <property name="serviceClass" value="net.swa.dewei.service.ws.HelloWorld"/>  
      <property name="address" value="http://localhost:8080/dewei2/services/HelloWorld"/>  
    </bean>  
    
    <bean id="deweiServiceClient" class="net.swa.dewei.service.ws.DeweiService"   
      factory-bean="helloWorldClientFactory" factory-method="create"/>  
      
    <bean id="deweiServiceClientFactory" class="org.apache.cxf.jaxws.JaxWsProxyFactoryBean">  
      <property name="serviceClass" value="net.swa.dewei.service.ws.DeweiService"/>  
      <property name="address" value="http://localhost:8080/dewei/services/DeweiService"/>  
    </bean>  

</beans>